{
  "name": "@eslib/lodash",
  "version": "0.1.0",
  "description": "(Lodash + ESlib = <3) All the Lodash functions available natively",
  "main": "dist/src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run clean && tsc -d",
    "clean": "rm -rf dist",
    "lint": "tslint .",
    "prepublish": "npm run lint && npm test",
    "pretest": "npm run build",
    "tdd": "concurrently -kr 'npm run watch' 'ava -w'",
    "test": "ava",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/eslib/lodash.git"
  },
  "author": "Boris Cherny <boris@performancejs.com> (https://performancejs.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eslib/lodash/issues"
  },
  "homepage": "https://github.com/eslib/lodash#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.68",
    "@types/node": "^8.0.10",
    "ava": "^0.20.0",
    "concurrently": "^3.5.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  },
  "dependencies": {
    "eslib": "^0.1.1",
    "lodash": "^4.17.4"
  },
  "ava": {
    "files": [
      "dist/test/*.js"
    ],
    "source": [
      "dist/src/*.js"
    ]
  }
}
